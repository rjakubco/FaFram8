<?xml version="1.0" encoding="UTF-8"?>
<fafram xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="parser/configuration.scheme.xsd">

	<framework>
		<fuse.group>testgroup</fuse.group>
		<fuse.id>testid</fuse.id>
		<fuse.version>testversion</fuse.version>
		<fuse.zip>testzip</fuse.zip>
		<fuse.path>testpath</fuse.path>
		<start.wait.time>999</start.wait.time>
		<stop.wait.time>999</stop.wait.time>
		<provision.wait.time>999</provision.wait.time>
		<patch.wait.time>999</patch.wait.time>
		<keepFolder>true</keepFolder>
		<patch>testpatch</patch>
		<fabric>testfabric</fabric>
		<fafram.working.directory>testdirectory</fafram.working.directory>
		<fafram.patch.dir>testdir</fafram.patch.dir>
	</framework>


	<!-- Framework settings settings -->
	<cluster>
		<!-- root default -->
		<global type="ROOT">
			<ssh host="openstack" username="fuse" password="fuse"/>
		</global>


	</cluster>





</fafram>
